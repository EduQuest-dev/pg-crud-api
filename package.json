{
  "name": "pg-crud-api",
  "version": "1.0.0",
  "type": "module",
  "description": "Auto-generated CRUD API for entire PostgreSQL database",
  "scripts": {
    "build": "node scripts/generate-build-info.js && tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "generate-key": "tsx src/auth/generate-key.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts' 'tests/**/*.ts'",
    "mcp": "node dist/mcp/index.js",
    "mcp:dev": "tsx src/mcp/index.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/swagger": "^9.6.0",
    "@fastify/swagger-ui": "^5.2.3",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "ajv-formats": "^3.0.1",
    "dotenv": "^16.4.7",
    "fastify": "^5.3.3",
    "pg": "^8.13.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.3",
    "neostandard": "^0.12.2",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
